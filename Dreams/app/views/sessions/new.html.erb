<h1>Sign In!</h1>

<form action=" <%= session_url %> " method="post">
  <%= auth_token %>

  <label for="user_username">
    username
    <input type="text" name="user[username]">
  </label>
  <br>
  <label for="user_password">
    password
    <input type="password" name="user[password]">
  </label>
  <br>
  <br>
  <input type="submit" value="Sign In">

</form>
